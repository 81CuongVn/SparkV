{
  "name": "spark.v",
  "version": "1.0.0",
  "description": "...",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "startweb": "node website/website.js",
    "s:dev": "doppler run -- node .",
    "sw:dev": "doppler run -- node website/website.js",
    "update": "git pull && npm install",
    "checkPacs": "npx npm-check-updates",
    "checkPacs:u": "npx npm-check-updates -u && npm install",
    "test": "eslint . --ignore-path .gitignore",
    "test:fix": "eslint . --ignore-path .gitignore --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "sass:s": "sass --watch assets/scss:assets/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingch1ll/sparkv.git"
  },
  "author": "KingCh1ll",
  "license": "MIT",
  "engines": {
    "node": "16.9.x",
    "npm": "7.x"
  },
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "@distube/soundcloud": "^0.2.2",
    "@distube/spotify": "^0.6.4",
    "@sentry/node": "^6.13.2",
    "@vitalets/google-translate-api": "^7.0.0",
    "aos": "^2.3.4",
    "ascii-table": "^0.0.9",
    "asciiart-logo": "^0.2.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.1.1",
    "canvacord": "^5.2.3",
    "canvas": "^2.8.0",
    "chalk": "^4.1.2",
    "connect-mongo": "^4.6.0",
    "dbots": "^9.0.1",
    "discord-api-types": "^0.23.1",
    "discord-canvas": "^1.4.1",
    "discord-giveaways": "^5.0.1",
    "discord-tictactoe": "^2.1.1",
    "discord-together": "^1.3.0",
    "discord-xp": "^1.1.16",
    "discord.js": "^13.1.0",
    "discord.js-akinator": "^3.2.1",
    "discordeasypages": "^0.2.4",
    "distube": "^3.0.0-beta.38",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "ffmpeg-static": "^4.4.0",
    "figlet": "^1.5.2",
    "libsodium-wrappers": "^0.7.9",
    "lyrics-finder": "^21.7.0",
    "markdown-table": "^2.0.0",
    "mongoose": "^6.0.7",
    "ms": "^2.1.3",
    "noblox.js": "^4.12.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.5.0",
    "passport-discord": "^0.1.4",
    "redis": "^3.1.2",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "statcord.js": "^3.3.0",
    "sweetalert2": "^11.1.7",
    "urban": "^0.3.4",
    "weather-js": "^2.0.0",
    "weky": "^3.1.8"
  },
  "devDependencies": {
    "ejs-lint": "^1.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "prettier": "2.4.1",
    "sass": "^1.42.1",
    "typings": "^2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "env": {
      "es2021": true,
      "node": true
    },
    "rules": {
      "no-await-in-loop": "off",
      "no-compare-neg-zero": "error",
      "no-template-curly-in-string": "error",
      "no-unsafe-negation": "error",
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false,
          "requireReturnDescription": false,
          "prefer": {
            "return": "returns",
            "arg": "param"
          },
          "preferType": {
            "String": "string",
            "Number": "number",
            "Boolean": "boolean",
            "Symbol": "symbol",
            "object": "Object",
            "function": "Function",
            "array": "Array",
            "date": "Date",
            "error": "Error",
            "null": "void"
          }
        }
      ],
      "no-unused-vars": "off",
      "accessor-pairs": "warn",
      "array-callback-return": "off",
      "consistent-return": "off",
      "curly": [
        "error",
        "multi-line",
        "consistent"
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "dot-notation": "error",
      "eqeqeq": "error",
      "no-redeclare": "off",
      "no-empty": "off",
      "no-empty-function": "off",
      "no-floating-decimal": "error",
      "no-implied-eval": "error",
      "no-invalid-this": "error",
      "no-lone-blocks": "error",
      "no-multi-spaces": "error",
      "no-new-wrappers": "error",
      "no-new": "off",
      "no-octal-escape": "error",
      "no-return-assign": "error",
      "no-return-await": "off",
      "no-self-compare": "error",
      "no-sequences": "off",
      "no-throw-literal": "error",
      "no-unmodified-loop-condition": "error",
      "no-unused-expressions": "off",
      "no-useless-call": "error",
      "no-useless-concat": "error",
      "no-useless-escape": "off",
      "no-useless-return": "error",
      "no-void": "error",
      "no-warning-comments": "warn",
      "prefer-promise-reject-errors": "error",
      "wrap-iife": "error",
      "yoda": "error",
      "no-label-var": "error",
      "no-shadow": "off",
      "no-undef": "off",
      "callback-return": "error",
      "getter-return": "off",
      "handle-callback-err": "off",
      "no-mixed-requires": "error",
      "no-path-concat": "error",
      "array-bracket-spacing": "error",
      "block-spacing": "error",
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "capitalized-comments": [
        "error",
        "always",
        {
          "ignoreConsecutiveComments": true
        }
      ],
      "comma-dangle": "off",
      "comma-spacing": "error",
      "comma-style": "error",
      "computed-property-spacing": "error",
      "eol-last": "error",
      "func-names": "off",
      "func-name-matching": "error",
      "func-style": "off",
      "key-spacing": "error",
      "keyword-spacing": "error",
      "max-depth": "off",
      "max-nested-callbacks": "off",
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "new-cap": "off",
      "newline-per-chained-call": [
        "error",
        {
          "ignoreChainWithDepth": 3
        }
      ],
      "no-array-constructor": "error",
      "no-inline-comments": "error",
      "no-lonely-if": "error",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2,
          "maxEOF": 1,
          "maxBOF": 0
        }
      ],
      "no-spaced-func": "error",
      "no-trailing-spaces": "error",
      "no-unneeded-ternary": "error",
      "no-whitespace-before-property": "error",
      "nonblock-statement-body-position": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "operator-assignment": "error",
      "padded-blocks": [
        "error",
        "never"
      ],
      "quote-props": [
        "error",
        "as-needed"
      ],
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "semi-spacing": "error",
      "semi": "error",
      "space-before-blocks": "error",
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "space-in-parens": "error",
      "space-infix-ops": "error",
      "space-unary-ops": "error",
      "spaced-comment": "error",
      "template-tag-spacing": "error",
      "unicode-bom": "error",
      "arrow-body-style": "error",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "arrow-spacing": "error",
      "no-duplicate-imports": "error",
      "no-useless-computed-key": "error",
      "no-useless-constructor": "error",
      "prefer-arrow-callback": "error",
      "prefer-numeric-literals": "error",
      "prefer-rest-params": "error",
      "prefer-spread": "error",
      "prefer-template": "error",
      "rest-spread-spacing": "error",
      "template-curly-spacing": "error",
      "yield-star-spacing": "error"
    }
  }
}
