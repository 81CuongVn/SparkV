  <!-- Loading Screen -->
  <style>
    #load {
      position: fixed;
      height: 100%;
      width: 100%;
      display: flex;
      align-items: center;
      justify-content: center;
      background-color: var(--sparkv-dark);
      z-index: 9999
    }

    .dots span {
      width: 25px;
      height: 25px;
      margin: 0 5px;
      border-radius: 50%;
      display: inline-block;
      background-color: var(--sparkv-yellow);
      animation-name: dots;
      animation-duration: 2s;
      animation-iteration-count: infinite;
      animation-timing-function: ease-in-out;
    }

    .dots span:nth-child(2) {
      animation-delay: 0.4s;
    }

    .dots span:nth-child(3) {
      animation-delay: 0.8s;
    }

    @keyframes dots {
      50% {
        opacity: 0;
        transform: scale(.7) translateY(10px)
      }
    }
  </style>