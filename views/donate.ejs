  <!-- Features -->
  <section id="features" class="overflow-hidden aos-init aos-animate">
    <div class="row flex-lg-row-reverse align-items-center g-5 py-5 px-5" data-aos="zoom-in">
      <div class="col-10 col-sm-8 col-lg-6">
        <img data-aos="fade-up" src="assets/images/sparkv_music.WebP" class="d-block mx-lg-auto img-fluid"
          alt="Bootstrap Themes" width="400" height="300" loading="lazy">
      </div>
      <div class="col-lg-6">
        <h1 class="display-5 fw-bold lh-1 mb-3" data-aos="fade-up">
          <a style="color: #3c57f3">Quality</a> Music
        </h1>
        <p class="lead" data-aos="fade-up">SparkV has <a style="color: blue">high quality</a> music. Perfect for karaoke
          night!</p>
      </div>
    </div>
    <div class="row flex-lg-row-reverse align-items-center g-5 py-5 px-5">
      <div class="col-lg-6">
        <h1 data-aos="fade-up" class="display-5 fw-bold lh-1 mb-3">Fun & Games</h1>
        <p data-aos="fade-up" class="lead">SparkV contains many fun/game commands that will keep your server active!</p>
      </div>
      <div class="col-10 col-sm-8 col-lg-6">
        <img data-aos="fade-up" src="assets/images/gaming.svg" class="d-block mx-lg-auto img-fluid"
          alt="Bootstrap Themes" width="400" height="300" loading="lazy">
      </div>
    </div>
  </section>

