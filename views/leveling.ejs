  <script type="module">
    if ('serviceWorker' in navigator) {
      navigator.serviceWorker
          .register('./service-worker.js')
          .then(() => console.log("⚙️ | Service Worker - Now ready for offline."));
    };
  </script> -->

  <%- include("components/scripts") %>
</body>

</html>